@inproceedings{weberFarsideSeismicSuite2022,
  title = {Farside {{Seismic Suite}} ({{FSS}}): {{Lunar Seismology}} in the {{Artemis Era}}},
  booktitle = {{{AGU Fall Meeting}} 2022},
  author = {Weber, Renee C. and Panning, Mark P. and Kedar, Sharon and Bowles, Neil and Calcutt, Simon B. and Drilleau, Melanie and Garcia, Raphael and Kawamura, Taichi and Lognonn{\'e}, Philippe Henri and Mimoun, David and Nunn, Ceri and Pike, William T. and De Raucourt, Sebastien and {Portela-Moreira}, Dilan and Wilhelm, Arnaud},
  year = 2022,
  month = dec,
  address = {Chicago, IL},
  abstract = {On the last day of the fiscal year in 1977, funding for the Apollo Passive Seismic Experiment ran out. The four seismometers, which had been operating continuously since each station was installed during the Apollo 12, 14, 15, and 16 missions (between 1969 and 1972), were switched off, and our seismic window into the lunar interior has been closed ever since. Understanding the formation and evolution of the Moon via a direct interrogation of its internal structure is a high-priority science objective in both the current planetary decadal survey, and in NASA's Artemis Plan for human exploration. Towards that objective, NASA is preparing to re-open its window into the Moon's interior, by funding the development of the Farside Seismic Suite (FSS) under the PRISM (Payloads and Research Investigations on the Surface of the Moon) program. FSS will be the first seismic package delivered to the Moon's farside. It will launch in 2025, on a lander provided by Draper Laboratory as part of NASA's Commercial Lunar Payload Services (CLPS) program, and will touch down in Schr{\"o}dinger Basin, one of the youngest impact basins on the Moon. FSS consists of two seismometers, both flight-proven on the Mars InSight mission: the vertical Very BroadBand (VBB) seismometer (the most sensitive seismometer ever built), and the Short Period (SP) sensor, the most mature compact triaxial sensor available for space applications. They are packaged together as a self-sufficient, lander-mounted payload that can survive independently, with power, communications and thermal control allowing continuous operation over multiple lunations. Thus FSS will outlive the delivery lander, and provide a long-lived seismic experiment capable of investigating deep lunar structure and the difference between near and farside activity; understanding how the lunar crust is affected by the development of an impact melt basin; and evaluating the current micrometeorite impact rate and local tectonic activity (with important implications for the Artemis human exploration program). By assessing the need for deployment and characterizing the lander seismic noise environment, FSS will also allow for key technical advancement and risk reduction for the future Lunar Geophysical Network mission, a candidate for NASA's New Frontiers 5 opportunity.}
}

@article{whizinSpectralMineralogicalAnalysis2023,
  title = {Spectral and {{Mineralogical Analysis}} of {{Planned CLPS Landing Sites}} with {{Lro-lamp}}},
  author = {Whizin, Akbar and Dolgas, Elena and Kurt, Retherford and Cjaika, Elizabeth and Greathouse, Thomas},
  year = 2023,
  month = oct,
  volume = {55},
  pages = {101.06},
  urldate = {2025-01-02},
  abstract = {In anticipation of the Commercial Lunar Payload Services (CLPS) landed missions to regions of interest on the Moon as well as the lunar south polar region, we performed a series of spectral analyses and created FUV maps for each candidate region. The Lunar Reconnaissance Orbiter's UV Lyman {$\alpha$} Mapping Project (LAMP) instrument continues to collect high spatial and spectral resolution maps of the lunar surface into its fifth extended mission. We will report LAMP imagery for these sites to aid in planning and upcoming data interpretation. Planned sites include the south pole, the Gruithuisen Domes, Reiner Gamma, and Mare Crisium. We compared LAMP data with ultraviolet, visible, and near-infrared spectral observations from two previous lunar missions, Clementine, and Kaguya as well as the most mineralogically and geologically relevant returned Apollo sample (and one Luna 24 sample). We produce Off/On-band albedo ratio maps with good signal to noise using the LAMP spectral mapping pipeline. Multiple regions of interest (ROIs) are selected at each site for comparison in both LAMP and previous mission data. Mare Crisium is more uniform in the FUV with some melt features, while the Gruithuisen Domes and Reiner Gamma both exhibit high FUV albedo ratios, pointing towards high feldspathic compositions (such as plagioclase) and space weathering. Melt features are highlighted and compared between the regions with contrasting compositions indicating differing subsurface lithologies and formation histories.},
  annotation = {ADS Bibcode: 2023DPS....5510106W}
}

