  author = {Saeidfirozeh, H. and Kubel{\'i}k, P. and Laitl, V. and K{\v r}ivkov{\'a}, A. and Vr{\'a}bel, J. and Rammelkamp, K. and Schr{\"o}der, S. and Gornushkin, I. B. and K{\'e}pe{\v s}, E. and {\v Z}abka, J. and Ferus, M. and Po{\v r}{\'i}zka, P. and Kaiser, J.},
