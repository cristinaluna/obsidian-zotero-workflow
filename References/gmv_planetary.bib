  author = {Lovell, Lewis and Adriani, Isabella C. and Nodjoumi, Giacomo and {Suarez-Valencia}, Javier E. and Le Corre, Daniel and Heward, Anita and Rossi, Angelo Pio and Cox, Nick L. J.},
  year = 2024,
