@article{bellMarsScienceLaboratory2017,
  title = {The {{Mars Science Laboratory}} {{{\emph{Curiosity}}}} Rover {{Mastcam}} Instruments: {{Preflight}} and In-flight Calibration, Validation, and Data Archiving},
  shorttitle = {The {{Mars Science Laboratory}} {{{\emph{Curiosity}}}} Rover {{Mastcam}} Instruments},
  author = {Bell, J. F. and Godber, A. and McNair, S. and Caplinger, M. A. and Maki, J. N. and Lemmon, M. T. and Van Beek, J. and Malin, M. C. and Wellington, D. and Kinch, K. M. and Madsen, M. B. and Hardgrove, C. and Ravine, M. A. and Jensen, E. and Harker, D. and Anderson, R. B. and Herkenhoff, K. E. and Morris, R. V. and Cisneros, E. and Deen, R. G.},
  year = 2017,
