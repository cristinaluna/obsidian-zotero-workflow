@misc{ottTrajectoryOptimizationAdaptive2024,
  title = {Trajectory {{Optimization}} for {{Adaptive Informative Path Planning}} with {{Multimodal Sensing}}},
  author = {Ott, Joshua and Balaban, Edward and Kochenderfer, Mykel},
  year = 2024,
