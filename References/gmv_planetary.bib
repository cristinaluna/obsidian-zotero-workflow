@article{tabernikSegmentationbasedDeeplearningApproach2020,
  title = {Segmentation-Based Deep-Learning Approach for Surface-Defect Detection},
  author = {Tabernik, Domen and {\v S}ela, Samo and Skvar{\v c}, Jure and Sko{\v c}aj, Danijel},
  year = 2020,
  journal = {Journal of Intelligent Manufacturing},
  volume = {31},
  number = {3},
  issn = {15728145},
  doi = {10.1007/s10845-019-01476-x},
  abstract = {Automated surface-anomaly detection using machine learning has become an interesting and promising area of research, with a very high and direct impact on the application domain of visual inspection. Deep-learning methods have become the most suitable approaches for this task. They allow the inspection system to learn to detect the surface anomaly by simply showing it a number of exemplar images. This paper presents a segmentation-based deep-learning architecture that is designed for the detection and segmentation of surface anomalies and is demonstrated on a specific domain of surface-crack detection. The design of the architecture enables the model to be trained using a small number of samples, which is an important requirement for practical applications. The proposed model is compared with the related deep-learning methods, including the state-of-the-art commercial software, showing that the proposed approach outperforms the related methods on the specific domain of surface-crack detection. The large number of experiments also shed light on the required precision of the annotation, the number of required training samples and on the required computational cost. Experiments are performed on a newly created dataset based on a real-world quality control case and demonstrates that the proposed approach is able to learn on a small number of defected surfaces, using only approximately 25--30 defective training samples, instead of hundreds or thousands, which is usually the case in deep-learning applications. This makes the deep-learning method practical for use in industry where the number of available defective samples is limited. The dataset is also made publicly available to encourage the development and evaluation of new methods for surface-defect detection.}
}

@article{tachikawaAdvancedPassiveThermal2022,
  title = {Advanced {{Passive Thermal Control Materials}} and {{Devices}} for {{Spacecraft}}: {{A Review}}},
  shorttitle = {Advanced {{Passive Thermal Control Materials}} and {{Devices}} for {{Spacecraft}}},
  author = {Tachikawa, Sumitaka and Nagano, Hosei and Ohnishi, Akira and Nagasaka, Yuji},
  year = 2022,
  month = jun,
  journal = {International Journal of Thermophysics},
  volume = {43},
  number = {6},
  pages = {91},
  issn = {0195-928X, 1572-9567},
  doi = {10.1007/s10765-022-03010-3},
  urldate = {2025-07-29},
  abstract = {Abstract             In recent planetary exploration space missions, spacecraft are exposed to severe thermal environments that are sometimes more extreme than those experienced in earth orbits. The development of advanced thermal control materials and devices together with reliable and accurate measurements of their thermophysical properties are needed for the development of systems designed to meet the engineering challenges associated with these space missions. We provide a comprehensive review of the state-of-the-art advanced passive thermal control materials and devices that are available for space applications, specifically, variable emissivity thermal control materials and microelectromechanical systems (MEMS), radiofrequency (RF)-transparent and/or tunable solar absorptivity and total hemispherical emissivity thermal control materials, and a passive re-deployable radiator with advanced materials and insulation. Prior to our in-depth review of these thermal control materials, we briefly summarize the thermal environments surrounding spacecraft, the characteristics of thermophysical properties for spacecraft materials that differ from those of materials for ground use, and the significance of solar absorptivity and total hemispherical emissivity for passive thermal control in space. In all four topics of materials and devices, the following subjects are overviewed: the basic principle of passive thermal control techniques in space, the measurement of thermophysical properties of those novel materials, simulation and/or on-orbit verification thermal performance tests, degradation tests in space environments, and some aspects of the implementation of the above-described materials and devices in actual space missions.},
  langid = {english},
  file = {C\:\\Users\\clsn\\Zotero\\storage\\26ITCJ9T\\Tachikawa et al. - 2022 - Advanced Passive Thermal Control Materials and Devices for Spacecraft A Review.pdf;C\:\\Users\\clsn\\Zotero\\storage\\B3APCRDD\\s10765-022-03010-3.pdf}
