@article{smithImagerMarsPathfinder1997,
  title = {The Imager for {{Mars Pathfinder}} Experiment},
  author = {Smith, P. H. and Tomasko, M. G. and Britt, D. and Crowe, D. G. and Reid, R. and Keller, H. U. and Thomas, N. and Gliem, F. and Rueffer, P. and Sullivan, R. and Greeley, R. and Knudsen, J. M. and Madsen, M. B. and Gunnlaugsson, H. P. and Hviid, S. F. and Goetz, W. and Soderblom, L. A. and Gaddis, L. and Kirk, R.},
  year = 1997,
  journal = {Journal of Geophysical Research: Planets},
  volume = {102},
  number = {E2},
  pages = {4003--4025},
  issn = {2156-2202},
  doi = {10.1029/96JE03568},
  urldate = {2024-12-17},
  abstract = {The imager for Mars Pathfinder (IMP), a stereoscopic, multispectral camera, is described in terms of its capabilities for studying the Martian environment. The camera's two eyes, separated by 15.0 cm, provide the camera with range-finding ability. Each eye illuminates half of a single CCD detector with a field of view of 14.4{\texttimes}14.0{$^\circ$} and has 12 selectable filters. The {\textflorin}/18 optics have a large depth of field, and no focussing mechanism is required; a mechanical shutter is avoided by using the frame transfer capability of the 512{\texttimes}512 CCD. The resolving power of the camera, 0.98 mrad/pixel, is approximately the same as the Viking Lander cameras; however, the signal-to-noise ratio for IMP greatly exceeds Viking, approaching 350. This feature along with the stable calibration of the filters between 440 and 1000 nm distinguishes IMP from Viking. Specially designed targets are positioned on the Lander; they provide information on the magnetic properties of wind-blown dust, measure the wind vectors, and provide radiometric standard reflectors for calibration. Also, eight low-transmission filters are included for imaging the Sun directly at multiple wavelengths, giving IMP the ability to measure dust opacity and potentially the water vapor content. Several experiments beyond the requisite color panorama are described in detail: contour mapping of the local terrain, multispectral imaging of the surrounding rock and soil to study local mineralogy, viewing of three wind socks, measuring atmospheric opacity and water vapor content, and estimating the magnetic properties of wind-blown dust. This paper is intended to serve as a guide to understanding the scientific integrity of the IMP data that will be returned from Mars starting on July 4, 1997.},
  copyright = {Copyright 1997 by the American Geophysical Union.},
  langid = {english},
  file = {C\:\\Users\\clsn\\Zotero\\storage\\4MHM8P52\\Smith et al. - 1997 - The imager for Mars Pathfinder experiment.pdf;C\:\\Users\\clsn\\Zotero\\storage\\DRWLGFTY\\Journal of Geophysical Research  Planets - 1997 - Smith - The imager for Mars Pathfinder experiment.pdf;C\:\\Users\\clsn\\Zotero\\storage\\2CH5IECV\\96JE03568.html}
}

