@article{higaVisionBasedEstimationDriving2019,
  title = {Vision-{{Based Estimation}} of {{Driving Energy}} for {{Planetary Rovers Using Deep Learning}} and {{Terramechanics}}},
  author = {Higa, Shoya and Iwashita, Yumi and Otsu, Kyohei and Ono, Masahiro and Lamarre, O. and Didier, Annie and Hoffmann, Mark},
