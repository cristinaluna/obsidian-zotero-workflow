@article{basilevskyClarificationSourcesMaterial2013,
  title = {Clarification of Sources of Material Returned by {{Luna}} 24 Spacecraft Based on Analysis of New Images of the Landing Site Taken by Lunar Reconnaissance Orbiter},
  author = {Basilevsky, A. T. and Ivanov, B. A. and Ivanov, A. V. and Head, J. W.},
