@article{dobreaRoverScienceAutonomy2025,
  title = {Rover {{Science Autonomy}} in {{Planetary Exploration}}: {{Field Analog Tests}}},
  shorttitle = {Rover {{Science Autonomy}} in {{Planetary Exploration}}},
  author = {Dobrea, Eldar Z. Noe and Banks, Maria E. and Clark, Roger N. and Wettergreen, David and Candela, Alberto and Hendrix, Amanda and Ahrens, Caitlin and Bell, Ernie and Breitfeld, Abigail and Bristow, Thomas F. and Buxner, Sanlyn and Hansen, Margaret and Holsclaw, Gregory M. and Knightly, Paul and Kramer, Georgiana and Kumari, Nandita and Lane, Melissa D. and Martin, Audrey and L. Meier, McKayla and Patterson, Ruby and Pearson, Neil and Prettyman, Thomas and Swayze, Gregg A. and Vaniman, David and Vijayarangan, Srinivasan and Vilas, Faith and Wright, Shawn P.},
